{
  "manifest_version": 3,
 
  "name": "Clockify extender",
  "action": {},
  "version": "0.1",
  "description": "Extend clockify with various nice things.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "appendDecimalHours.js"
  },
  "content_scripts": [{
    "css": ["styles.css"],
    "matches": [
      "https://clockify.me/*"
    ]
  }]
}